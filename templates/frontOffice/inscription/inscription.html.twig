{% block head %}

        <title>Inscription</title>
        <meta charset="utf-8">
        <meta http-equiv="X-UA-Compatible" content="IE=edge">
        <meta name="viewport" content="width=device-width, initial-scale=1.0">
        <meta http-equiv="cache-control" content="no-cache">
        <meta http-equiv="expires" content="Thu, 19 Nov 1900 08:52:00 GMT"><!--Loading bootstrap css-->
        <link type="text/css"
              href="../../../fonts.googleapis.com/css638b.css?family=Open+Sans:400italic,700italic,800italic,400,700,800">
    <link type="text/css" rel="stylesheet"
          href="{{ asset('backOffice/vendors/jquery-ui-1.10.3.custom/css/ui-lightness/jquery-ui-1.10.3.custom.html') }}">
    <link type="text/css" rel="stylesheet" href="{{ asset('backOffice/vendors/font-awesome/css/font-awesome.min.css') }}">
    <link type="text/css" rel="stylesheet" href="{{ asset('backOffice/vendors/bootstrap/css/bootstrap.min.css') }}"><!--Loading style vendors-->
    <link type="text/css" rel="stylesheet" href="{{ asset('backOffice/vendors/animate.css/animate.css') }}">
    <link type="text/css" rel="stylesheet" href="{{ asset('backOffice/vendors/iCheck/skins/all.css') }}"><!--Loading style-->
    <link type="text/css" rel="stylesheet" href="{{ asset('backOffice/css/themes/style1/pink-blue.css') }}" class="default-style">
    <link type="text/css" rel="stylesheet" href="{{ asset('backOffice/css/themes/style1/pink-blue.css') }}" id="theme-change"
          class="style-change color-change">
    <link type="text/css" rel="stylesheet" href="{{ asset('backOffice/css/style-responsive.css') }}">
    <link rel="shortcut icon" href="{{ asset('backOffice/images/favicon.html') }}">

{% endblock %}


{% block body %}
    <body id="signup-page">
    <div class="page-form">
        <form id="signup-form"  class="form" method="post" enctype="multipart/form-data">
            <div class="header-content"><h1>Inscription</h1></div>
            <div class="body-content">
                <div style="margin-bottom: 15px" class="row">
                    <div class="col-lg-6">
                        <label>
                            {{ form_widget(form.nom, {'attr' : {'placeholder' : "Nom"}}) }}
                        </label>
                    </div>
                    <div class="col-lg-6">
                        <label>
                            {{ form_widget(form.prenom, {'attr' : {'placeholder' : "Prenom"}}) }}
                        </label>
                    </div>
                </div>
                <hr>
                <div class="form-group">
                    <div class="input-icon right"><i class="fa fa-envelope"></i>
                        {{ form_widget(form.email, {'attr' : {'placeholder' : "Adresse email"}}) }}
                    </div>
                </div>

                <div class="form-group">
                    <div class="input-icon right"><i class="fa fa-user"></i>
                        {{ form_widget(form.entreprise, {'attr' : {'placeholder' : "Entreprise"}}) }}
                    </div>
                </div>
                <div class="form-group">
                    <div class="input-icon right"><i class="fa fa-user"></i>
                        {{ form_widget(form.specialisation, {'attr' : {'placeholder' : "Specialisation"}}) }}

                    </div>
                </div>
                <div class="form-group">
                    <div class="input-icon right"><i class="fa fa-user"></i>
                        {{ form_widget(form.fonction, {'attr' : {'placeholder' : "Fonction"}}) }}

                    </div>
                </div>
                <div class="form-group">
                    <div class="input-icon right"><i class="fa fa-key"></i>
                        {{ form_widget(form.plainPassword.first, {'attr' : {'placeholder' : "Mot de passe"}}) }}
                     </div>
                </div>
                <div class="form-group">
                    <div class="input-icon right"><i class="fa fa-key"></i>
                        {{ form_widget(form.plainPassword.second, {'attr' : {'placeholder' : "Confirmer le mot de passe"}}) }}
                    </div>
                </div>
                    {{ form_rest(form) }}
                <div class="form-group">
                    <div class="checkbox-list"><label><input id="subscription" type="checkbox" name="subscription">&nbsp;
                            I want to receive news and special offers</label></div>
                </div>
                <div class="form-group">
                    <div class="checkbox-list"><label><input id="terms" type="checkbox" name="terms">&nbsp;
                            I agree with the Terms and Conditions</label></div>
                </div>
                <hr>
                <div class="form-group mbn"><a href="extra-signin.html" class="btn btn-warning"><i
                                class="fa fa-chevron-circle-left"></i>&nbsp;
                        Retour</a>
                    <button type="submit" class="btn btn-info pull-right">S'inscrire
                        &nbsp;<i class="fa fa-chevron-circle-right"></i></button>
                </div>
            </div>
        </form>
    </div>
    <script src="{{ asset('backOffice/js/jquery-1.10.2.min.js') }}"></script>
    <script src="{{ asset('backOffice/js/jquery-migrate-1.2.1.min.js') }}"></script>
    <script src="{{ asset('backOffice/js/jquery-ui.js') }}"></script><!--loading bootstrap js-->
    <script src="{{ asset('backOffice/vendors/bootstrap/js/bootstrap.min.js') }}"></script>
    <script src="{{ asset('backOffice/vendors/bootstrap-hover-dropdown/bootstrap-hover-dropdown.js') }}"></script>
    <script src="{{ asset('backOffice/vendors/jquery-validate/jquery.validate.min.js') }}"></script>
    <script src="{{ asset('backOffice/js/html5shiv.js') }}"></script>
    <script src="{{ asset('backOffice/js/respond.min.js') }}"></script>
    <script src="{{ asset('backOffice/js/extra-signup.js') }}"></script>
    <script src="{{ asset('backOffice/vendors/iCheck/custom.min.js') }}"></script>
    <script src="{{ asset('backOffice/vendors/iCheck/custom.min.js') }}"></script>
    <script>//BEGIN CHECKBOX & RADIO
        $('input[type="checkbox"]').iCheck({
            checkboxClass: 'icheckbox_minimal-grey',
            increaseArea: '20%' // optional
        });
        $('input[type="radio"]').iCheck({
            radioClass: 'iradio_minimal-grey',
            increaseArea: '20%' // optional
        });
        //END CHECKBOX & RADIO</script>
    <script type="text/javascript">(function (i, s, o, g, r, a, m) {
            i['GoogleAnalyticsObject'] = r;
            i[r] = i[r] || function () {
                (i[r].q = i[r].q || []).push(arguments)
            }, i[r].l = 1 * new Date();
            a = s.createElement(o),
                m = s.getElementsByTagName(o)[0];
            a.async = 1;
            a.src = g;
            m.parentNode.insertBefore(a, m)
        })(window, document, 'script', '../../../www.google-analytics.com/analytics.js', 'ga');
        ga('create', 'UA-145464-12', 'auto');
        ga('send', 'pageview');</script>
    </body>



{{ form_end(form) }}
{% endblock %}
